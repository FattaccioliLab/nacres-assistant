function processPdfFromDrive() {
  const ui  = SpreadsheetApp.getUi();
  const res = ui.prompt(
    "PDF from Google Drive",
    "Paste the Google Drive file URL or file ID:",
    ui.ButtonSet.OK_CANCEL
  );
  if (res.getSelectedButton() !== ui.Button.OK) return;

  const input = res.getResponseText().trim();
  if (!input) return;

  let fileId = input;
  const match = input.match(/[-\w]{25,}/);
  if (match) fileId = match[0];

  let file;
  try { file = DriveApp.getFileById(fileId); }
  catch(e) { ui.alert("âŒ Could not access file.\n" + e.message); return; }

  if (file.getMimeType() !== "application/pdf") { ui.alert("âŒ File is not a PDF."); return; }

  const pdfBase64 = Utilities.base64Encode(file.getBlob().getBytes());

  try {
    const items = classifyItems(null, pdfBase64);
    writeResultsToSheet(items);
    SpreadsheetApp.getActive().getSheetByName(SHEET_OUTPUT).activate();
    ui.alert("âœ… Done! " + items.length + " items found.\n\nReview column G and correct if needed.");
  } catch(e) {
    SpreadsheetApp.getActiveSpreadsheet().toast("", "ðŸ§ª NACRES", 1);
    ui.alert("âŒ Error: " + e.message);
  }
}


