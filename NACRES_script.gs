// ============================================================
//  CLAUDE API HELPERS
// ============================================================
function callClaude(apiKey, payload) {
  const response = UrlFetchApp.fetch("https://api.anthropic.com/v1/messages", {
    method: "post",
    headers: {
      "x-api-key": apiKey,
      "anthropic-version": "2023-06-01",
      "content-type": "application/json"
    },
    payload: JSON.stringify(payload),
    muteHttpExceptions: true
  });

  const code = response.getResponseCode();
  const body = response.getContentText();
  if (code !== 200) throw new Error("Claude API error " + code + ": " + body);

  const json = JSON.parse(body);
  return {
    text:   json.content[0].text.trim(),
    input:  json.usage ? json.usage.input_tokens  : 0,
    output: json.usage ? json.usage.output_tokens : 0
  };
}

// Claude Haiku 4.5 pricing: $1 / 1M input tokens, $5 / 1M output tokens
function estimateCost(inputTokens, outputTokens) {
  const costUsd = (inputTokens / 1e6) * 1.00 + (outputTokens / 1e6) * 5.00;
  if (costUsd < 0.0001) return "< $0.0001";
  return "$" + costUsd.toFixed(4);
}

function parseJsonArray(text) {
  const clean = text.replace(/^```json\s*/i, "").replace(/```\s*$/i, "").trim();
  let parsed;
  try { parsed = JSON.parse(clean); }
  catch(e) { throw new Error("Could not parse Claude response as JSON.\n\nRaw:\n" + text.substring(0, 500)); }
  if (!Array.isArray(parsed)) throw new Error("Expected a JSON array, got: " + typeof parsed);
  return parsed;
}


